<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ttc.polytechnicshiksha">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <queries>
        <intent>
            <action android:name="com.google.android.youtube.api.service.START" />
        </intent>
    </queries>
    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <data
                android:host="youtube.com"
                android:scheme="https" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logohd"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logohd"
        android:supportsRtl="true"
        android:theme="@style/Theme.PolytechnicELearning"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".enotes"
            android:exported="false"
            android:label="@string/title_activity_enotes"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".campus_placement"
            android:exported="true"
            android:label="@string/title_activity_campus_placement"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".Dashboard_College"
            android:label="@string/title_activity_dashboard__college"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".preview_tpo"
            android:label="@string/title_activity_preview_tpo"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".TPO_college"
            android:label="@string/title_activity_t_p_o_college"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".login"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".disclaimer"
            android:label="@string/title_activity_disclaimer"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".contact_us"
            android:label="@string/title_activity_contact_us"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".syllabus"
            android:label="@string/title_activity_syllabus"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".imp_link"
            android:label="@string/title_activity_imp_link"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".qpsolution"
            android:label="@string/title_activity_qpsolution"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity android:name=".workinprogress" />
        <activity
            android:name=".MainActivity2"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".mcq"
            android:label="@string/title_activity_mcq"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".electure"
            android:label="@string/title_activity_electure"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".lab_manuals"
            android:label="@string/title_activity_lab_manuals"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar" />
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/Theme.PolytechnicELearning.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>